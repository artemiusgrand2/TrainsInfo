<?xml version="1.0" encoding="utf-8"?>
<Configuration xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <Logger>
    <Type>log4net</Type>
    <Settings>
      <Setting Key="ConfigFilePath" Value=".\Log4NetConfig.xml"/>
      <Setting Key="LogLevel" Value="Low"/>
    </Settings>
  </Logger>
  
  
  <Listeners>
 
   <Listener>
      <Type>Http</Type>
      <CommunicationProtocol>Dsccp</CommunicationProtocol>
      <Settings>
        <Setting Key="BindPort" Value="8888"/>
      </Settings>
    </Listener>

  </Listeners>

  <DataSources>

    <DataSource>
      <Name>CountWagonInJointSource</Name>
      <DataParsers>
	    <DataParser>JointAllVagon</DataParser>
	  </DataParsers>
      <RequestTimeout>1000</RequestTimeout>
      <DataStream>
        <Type>File</Type>
        <ConnectionString>w:\OperNalVag.csv</ConnectionString>
      </DataStream>
    </DataSource>
	
	  <DataSource>
      <Name>JointFactSource</Name>
      <DataParsers>
	    <DataParser>AllFactTrains</DataParser>
	  </DataParsers>
      <RequestTimeout>1000</RequestTimeout>
      <DataStream>
        <Type>File</Type>
        <ConnectionString>w:\MejGosStiki.csv</ConnectionString>
      </DataStream>
    </DataSource>
	
	 <DataSource>
      <Name>IAS_PYR_GPSource</Name>
      <DataParsers>
	    <DataParser>NodeOT</DataParser>
		<DataParser>NodeRF</DataParser>
		<DataParser>NodeTR</DataParser>
		<DataParser>AreaGargo</DataParser>
		<DataParser>AreaPass</DataParser>
	  </DataParsers>
      <RequestTimeout>10000</RequestTimeout>
      <DataStream>
        <Type>IAS_PYR_GP</Type>
        <ConnectionString>Data Source=10.20.47.53\INFBOARD;Initial Catalog=Iaspurgp;User ID=sa;Password=111111111111111</ConnectionString>
      </DataStream>
    </DataSource>
	
	
	<!-- разбор таблиц -->
	
	  <DataSource>
      <Name>PassOpzSource</Name>
      <DataParsers>
	    <DataParser>PassOpz</DataParser>
	  </DataParsers>
      <RequestTimeout>1000</RequestTimeout>
      <DataStream>
        <Type>FileZip</Type>
        <ConnectionString>w:\PassOpz.zip</ConnectionString>
      </DataStream>
    </DataSource>
	
	
	  <DataSource>
      <Name>Vostok_ZapadSource</Name>
      <DataParsers>
	    <DataParser>Vostok_Zapad</DataParser>
	  </DataParsers>
      <RequestTimeout>1000</RequestTimeout>
      <DataStream>
        <Type>FileZip</Type>
        <ConnectionString>w:\Vostok_Zapad.zip</ConnectionString>
      </DataStream>
    </DataSource>
	
	  <DataSource>
      <Name>Zapad_VostokSource</Name>
      <DataParsers>
	    <DataParser>Zapad_Vostok</DataParser>
	  </DataParsers>
      <RequestTimeout>1000</RequestTimeout>
      <DataStream>
        <Type>FileZip</Type>
        <ConnectionString>w:\Zapad_Vostok.zip</ConnectionString>
      </DataStream>
    </DataSource>

  </DataSources>


  <DataStorage>
    <ConnectionString>Data Source=10.20.47.53\INFBOARD;Initial Catalog=Tablo;User ID=sa;Password=111111111111111</ConnectionString >
